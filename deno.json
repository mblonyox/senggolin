{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "esm:add": "deno run -A https://esm.sh/v112 add",
    "esm:update": "deno run -A https://esm.sh/v112 update",
    "esm:remove": "deno run -A https://esm.sh/v112 remove"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}